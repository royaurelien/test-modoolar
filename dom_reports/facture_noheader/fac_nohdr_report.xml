<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="facture_nohdr">
        <t t-call="web.html_container">
            <t t-set="facture" t-value="True"/>
            <t t-set="no_header" t-value="True"/>
            <t t-set="no_footer" t-value="True"/>
            <t t-foreach="docs" t-as="doc">
                <t t-call="dom_reports.dom_report_invoice_document" t-lang="doc.partner_id.lang"/>
            </t>
        </t>
    </template>

</odoo>
