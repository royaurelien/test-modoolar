<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="dom_stock_quant_tree_inherit" model="ir.ui.view">
            <field name="name">Dom stock quant tree </field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='quantity']" position="after">
                    <field name="stock_value" sum="Stock Valuation" widget="monetary" string="Valeur total"/>
                </xpath>

            </field>
        </record>

    </data>
</openerp>


